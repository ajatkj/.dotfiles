	cup=\E[%i%p1%d;%p2%dH, cuu=\E[%p1%dA, cuu1=\E[A,
	dch=\E[%p1%dP, dch1=\E[P, dl=\E[%p1%dM, dl1=\E[M,
        enacs=\E(B\E)0,
	ed=\E[J, el=\E[K, el1=\E[1K$<3>, home=\E[H, ht=\t,
	hts=\EH, ich=\E[%p1%d@, ich1=\E[@, il=\E[%p1%dL,
